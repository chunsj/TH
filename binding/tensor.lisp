         (k (if k k (min nr nc)))
         (transposed nil))
    (when (< nr nc)
      (setf m ($transpose m)
            transposed T))
    (let* ((rm (rndn ($size m 1) k))
           (qr ($qr ($@ m rm)))
           (q (car qr))
