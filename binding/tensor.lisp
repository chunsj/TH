(defun print-3d (tensor stream)
  (let ((n0 ($ ($size tensor) 0)))
    (format stream "#<~A ~A>~%" (type-of tensor) ($size tensor))
    (loop :for i :from 0 :below (min 5 n0)
          :do (format stream "[~A,]~%~A" i ($ tensor i)))
    (if (>= n0 5)
        (format stream "[5,] ···~%"))))

