(defmethod $subview ((tensor tensor) &rest index-sizes)
  (let ((cx ($narrow tensor 0 (car index-sizes) (cadr index-sizes)))
        (dim 1))
    (loop :for (i s) :on (cddr index-sizes) :by #'cddr
          :do (progn ($narrow! cx dim i s)
                     (incf dim)))
    cx))

