  (if (eq 1 ($ndim tensor))
      ($reshape tensor ($count tensor) 1)
      (cond ((and (null dimension0) (null dimension1)) (tensor-new-transpose tensor))
            (t (let ((result ($empty tensor)))
                 (tensor-transpose result tensor dimension0 dimension1)
                 result)))))

