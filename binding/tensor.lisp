  (let ((n (min ($size storage) (tensor-n-element tensor))))
    (format stream "#<~A ~A [~{~11,4E~^ ~}~A>" (type-of tensor) ($size tensor)
            (loop :for i :from 0 :below (min 5 n) :collect ($ storage i))
            (if (>= n 5) " ···]" "]"))))

